{
    "manifest_version": 3,
    "name": "Procrastination Parrot",
    "description": "Base Level Extension",
    "version": "1.0",
    "permissions": [
        "activeTab", "tabs"
      ],
    "action": {
      "default_popup": "hello.html",
      "default_icon": "parrot.png"
    },
    "background": {
        "service_worker": "script.js"
      }
  }